<div class='contacts-header'>
  <h2>Students</h2>
</div>
<div class='contacts-panel'>
  <div class='contacts-list'>
    <h2>All Contacts:</h2>
    <ul>
      <% @contacts.each do |contact| %>
        <li><%= link_to contact.full_name, user_contact_path(contact.user, contact), remote: true %></li>

      <% end %>
    </ul>
    <span id='new-contact'><%= link_to 'Add New Contact', new_user_contact_path(@user), :remote => true %></span>
  </div>
</div>