<div id='students-list'>
  <div class='contacts-panel students-panel content-panel small-scroll'>
    <div class='contacts-list'>
      <ul class='contacts-list-append'>
        <% @contacts.each do |contact| %>
          <%= render 'student', contact: contact %></a>
        <% end %>
        <button class="contact-hover view" id="teacher-view">
          <h3 class="list-title contact-name">
            Back to Teacher View
          </h3>
        </button>
      </ul>
    </div>
  </div>
  <%= submit_tag 'Teacher View', :type => 'button', :id => "teacher-view" %>
</div>



