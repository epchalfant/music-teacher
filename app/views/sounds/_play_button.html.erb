    <%= form_tag(sc_play_path, :class => "play", :method => 'post', :remote => true, :id => track.id) do %>
      <%= image_tag track.artwork_url %><br>
      <%= hidden_field_tag 'track_url', track.permalink_url %>
      <%= submit_tag 'Play!'  %>
    <% end %>
    <%= submit_tag "Back!" class: "back", id: "b" track.id %>